{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "backend/vercel_entry.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/vercel_entry.py"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ]
  }